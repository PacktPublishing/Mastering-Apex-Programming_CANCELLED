<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>Check_AccountNumber_Length</fullName>
        <active>true</active>
        <description>To check  the length of Account Number</description>
        <errorConditionFormula>AND(NOT(ISBLANK(AccountNumber)),
LEN(AccountNumber)&gt;8)</errorConditionFormula>
        <errorDisplayField>AccountNumber</errorDisplayField>
        <errorMessage>Account Number should not be greater than 8 digits.</errorMessage>
    </validationRules>
</CustomObject>
